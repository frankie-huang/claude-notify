{
  "msg_type": "interactive",
  "card": {
    "header": {
      "template": "blue",
      "title": {
        "content": "{{title}}",
        "tag": "plain_text"
      }
    },
    "elements": [
      {
        "tag": "markdown",
        "content": "{{content}}",
        "text_align": "left",
        "text_size": "normal_v2",
        "margin": "0px 0px 0px 0px"
      },
      {
        "tag": "hr"
      },
      {
        "tag": "column_set",
        "columns": [
          {
            "tag": "column",
            "width": "weighted",
            "elements": [
              {
                "tag": "markdown",
                "content": "**项目：**\n{{project_name}}",
                "text_align": "left",
                "text_size": "normal_v2"
              }
            ],
            "vertical_align": "top",
            "weight": 1
          },
          {
            "tag": "column",
            "width": "weighted",
            "elements": [
              {
                "tag": "markdown",
                "content": "**时间：**\n{{timestamp}}",
                "text_align": "left",
                "text_size": "normal_v2"
              }
            ],
            "vertical_align": "top",
            "weight": 1
          }
        ]
      },
      {
        "tag": "note",
        "elements": [
          {
            "tag": "plain_text",
            "content": "请尽快返回处理"
          }
        ]
      }
    ]
  }
}